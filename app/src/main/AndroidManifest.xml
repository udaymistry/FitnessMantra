<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sonal.fitnessmantra" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Bmi_calcy"
            android:label="@string/title_activity_bmi_calcy" >
        </activity>
        <activity
            android:name=".Exercise"
            android:label="@string/title_activity_exercise" >
        </activity>
        <activity
            android:name=".seven_minitue_excercise"
            android:label="@string/title_activity_seven_minitue_excercise"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sonal.fitnessmantra.MainActivity" />
        </activity>
        <activity
            android:name=".exercise_view"
            android:label="@string/title_activity_exercise_view" >
        </activity>
    </application>

</manifest>
